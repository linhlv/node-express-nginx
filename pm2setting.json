{
  "apps":
  [
    {
      "name":"node-express-nginx",
      "script":"bin/www",
      "watch":["views", "public", "routes"],
      "env":{
        "NODE_ENV":"development"
      },
      "env_production":{
        "NODE_ENV":"production"
      },
      "log_date_format":"MM-DD-YYYY HH:mm Z",
      "error_file":"node-express-nginx-err.log",
      "out_file":"node-express-nginx-out.log"
    }
  ]
}
